{
  "name": "miaoxing/wechat-mp",
  "description": "Miaoxing WeChat Mini Program Plugin",
  "keywords": [
    "miaoxing",
    "wechat",
    "mini program"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.10.3",
    "miaoxing/user": "^0.5.24",
    "miaoxing/wechat": "^0.1.37",
    "miaoxing/payment": "^0.1.61"
  },
  "require-dev": {
    "miaoxing/dev": "^9.1.3"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\WechatMp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\WechatMp\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/admin": "miaoxing/admin as 0.15.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.15.x-dev",
      "miaoxing/services": "miaoxing/services as 0.9.x-dev",
      "wei/wei": "twinh/wei as 0.17.x-dev",
      "miaoxing/file": "miaoxing/file as 0.3.x-dev",
      "miaoxing/wechat": "miaoxing/wechat as 0.1.x-dev"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "@php wei g:auto-completion"
    ]
  }
}
