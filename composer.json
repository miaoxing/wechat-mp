{
  "name": "miaoxing/wechat-mp",
  "description": "Miaoxing WeChat Mini Program Plugin",
  "keywords": [
    "miaoxing", "wechat", "mini program"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.3",
    "miaoxing/user": "^0.2.17",
    "miaoxing/wechat": "^0.1.0"
  },
  "require-dev": {
    "miaoxing/dev": "^7.0.1"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\WechatMp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\WechatMp\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/plugin": "miaoxing/plugin as 0.4.x-dev",
      "miaoxing/services": "miaoxing/services as 0.3.x-dev",
      "wei/wei": "twinh/wei as 0.10.x-dev",
      "miaoxing/wechat": "miaoxing/wechat as 0.1.x-dev"
    }
  }
}
