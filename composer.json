{
  "name": "miaoxing/wechat-mp",
  "description": "Miaoxing WeChat Mini Program Plugin",
  "keywords": [
    "miaoxing",
    "wechat",
    "mini program"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.6.3",
    "miaoxing/user": "^0.4.5",
    "miaoxing/wechat": "^0.1.6",
    "miaoxing/payment": "^0.1.26"
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\WechatMp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\WechatMp\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/plugin": "miaoxing/plugin as 0.8.x-dev",
      "miaoxing/services": "miaoxing/services as 0.5.x-dev",
      "wei/wei": "twinh/wei as 0.12.x-dev",
      "miaoxing/file": "miaoxing/file as 0.3.x-dev",
      "miaoxing/wechat": "miaoxing/wechat as 0.1.x-dev"
    }
  }
}
